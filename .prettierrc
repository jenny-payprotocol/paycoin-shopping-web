{
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "auto",
    "singleAttributePerLine": true,
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "objectCurlyNewline": {
        "multiline": true,
        "minProperties": 3
    }
}
